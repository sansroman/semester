<p id="notice"><%= notice %></p>

<h1>Healths</h1>

<table>
  <thead>
    <tr>
      <th>Target</th>
      <th>Exercise</th>
      <th>Birthday</th>
      <th>Weight</th>
      <th>Height</th>
      <th>Step</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @healths.each do |health| %>
      <tr>
        <td><%= health.target %></td>
        <td><%= health.exercise %></td>
        <td><%= health.birthday %></td>
        <td><%= health.weight %></td>
        <td><%= health.height %></td>
        <td><%= health.step %></td>
        <td><%= health.user.email %></td>
        <td><%= link_to 'Show', health %></td>
        <td><%= link_to 'Edit', edit_health_path(health) %></td>
        <td><%= link_to 'Destroy', health, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Health', new_health_path %>
